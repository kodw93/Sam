     ]
    }
   ],
   "source": [
    "try:\n",
    "    a = int(input(\"Number 1: \"))\n",
    "    if a<0:\n",
    "        raise ValueError(\"Entered a Negative\")\n",
    "except ValueError:\n",
    "    print(\"Whoa, that's not an integer\")\n",
    "    raise"
   ]
