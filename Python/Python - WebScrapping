     "data": {
      "text/plain": [
       "list"
      ]
     },
     "execution_count": 37,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "nba_data_list = pd.read_html(\"https://www.basketball-reference.com/draft/NBA_2017.html\") \n",
    "type(nba_data_list)"
   ]
