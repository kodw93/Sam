ggplot() +
geom_line (mapping = aes(x= year, y=city_mpg), color = "red") +
geom_line (mapping = aes(x= year, y=highway_mpg), color = "blue") +
geom_line (mapping = aes(x= year, y=overall_mpg), color = "green") +
xlab("YEAR") + ylab("AVERAGE MPG")
